#include <stdio.h>
#include <sys/types.h>
#include <unistd.h>
int main() {
  printf("YAY!\n");
  pid_t pid = fork();
  if (pid == 0) {
    // I am the child
    printf("CHILD!\n");
    char *args [] = { 
      "/bin/ls",
      "-a",
      "-l",
      "-h", NULL};
    execv("/bin/ls", args);
  } else {
    printf("PARENT\n");
    execl("/bin/ls", "/bin/ls", "-i", "\"[a-z]\"", NULL);
  }

  printf("end\n");
}
